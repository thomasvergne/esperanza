module Main
  type Response = Response (status: int) (body: string)

  let withCode (code: int) (res: Response): Option (r: Response | status r == code) = do
    if status res == code 
      then Just res 
      else Nothing

  let main = do
    let 
      res  = Response 200 "Hello, world"
      res' = withCode 200 res 
    print (Response.show res')

